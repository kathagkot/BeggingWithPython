class Osoba:

    def __init__(self, imie, wiek):
        self.nazwisko = imie
        self.wiek = wiek
    def SayHello (self):
        print ("Czesc, jestem " + self.nazwisko +".")

    def Age (self):
        print ("Mam" + " " +  str(self.wiek) +" " + "lat.")
        
Janek = Osoba ("Kowalski", 20)
Janek.SayHello ()
Janek.Age()
